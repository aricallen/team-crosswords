{
  "name": "team-crosswords",
  "version": "0.0.1",
  "description": "solve a NYT crossword with others in realtime.",
  "main": "index.js",
  "scripts": {
    "dev": "babel --source-maps --watch src --out-dir dist",
    "dev:fetch": "nodemon pocs/fetch.js",
    "dev:express": "nodemon pocs/express.js",
    "dev:exphbs": "nodemon pocs/exphbs/app.js",
    "dev:websockets": "nodemon pocs/websockets/server.js",
    "dev:mongodb": "nodemon pocs/mongodb.js",
    "dev:mongodb-crud": "nodemon pocs/mongodb-crud.js",
    "test": "mocha tests/**/*.test.js -w --compilers js:babel-register",
    "build:browser": "babel --source-maps --watch tests/browser/src --out-dir tests/browser/dist"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2017": "^1.4.0",
    "chai": "^3.5.0",
    "express": "^4.15.2",
    "express-handlebars": "^3.0.0",
    "jshint": "^2.9.4",
    "mocha": "^2.4.5",
    "mongodb": "^2.2.26",
    "mongoose": "^4.9.6",
    "node-fetch": "^1.5.3",
    "nodemon": "^1.11.0",
    "promised-mongo": "^1.2.0",
    "socket.io": "^1.7.3"
